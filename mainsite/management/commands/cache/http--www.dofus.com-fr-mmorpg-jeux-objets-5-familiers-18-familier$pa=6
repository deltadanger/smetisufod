<div class="middle_content" id="mini-champion">
  <h2 class="title_element" onclick="OpenCloseDiv('bb8071');">Mini Champion</h2>  <div id="bb8071" class="forumbbcode" style="display:none;">
    <div class="h3" style="color:#000 !important">Forum :</div>
    <p><input onclick="javascript:this.select();" type="text" readonly="readonly" value="[dofusitem=8071]Mini Champion[/dofusitem]" /></p>
      </div>
  <span class="level_element">Level 1</span>
  <div class="element_desc">
    <div class="top"></div>
    <div class="middle">
      <div class="fix">
        <!--[if !IE]> -->
<object type="application/x-shockwave-flash" data="http://staticns.ankama.com/dofus/www//game/objectViewer.swf" id="F774d47151f5c0756877b13ac2a755bf7" width="120" height="120">
<!-- <![endif]-->
<!--[if IE]>
<object class="fix" classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version="10.0.0 id="F774d47151f5c0756877b13ac2a755bf7" width="120" height="120">
