<div class="middle_content" id="resinette">
  <h2 class="title_element" onclick="OpenCloseDiv('bb7002');">La Résinette</h2>  <div id="bb7002" class="forumbbcode" style="display:none;">
    <div class="h3" style="color:#000 !important">Forum :</div>
    <p><input onclick="javascript:this.select();" type="text" readonly="readonly" value="[dofusitem=7002]La Résinette[/dofusitem]" /></p>
      </div>
  <span class="level_element">Level 94</span>
  <div class="element_desc">
    <div class="top"></div>
    <div class="middle">
      <div class="fix">
        <!--[if !IE]> -->
<object type="application/x-shockwave-flash" data="http://staticns.ankama.com/dofus/www//game/objectViewer.swf" id="Fbc79fa83849da7c5af8bdb419155de36" width="120" height="120">
