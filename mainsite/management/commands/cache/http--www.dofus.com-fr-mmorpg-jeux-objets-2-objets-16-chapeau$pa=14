  <h2 class="title_element" onclick="OpenCloseDiv('bb7227');">Chapeau Terrdala</h2>  <div id="bb7227" class="forumbbcode" style="display:none;">
    <div class="h3" style="color:#000 !important">Forum :</div>
    <p><input onclick="javascript:this.select();" type="text" readonly="readonly" value="[dofusitem=7227]Chapeau Terrdala[/dofusitem]" /></p>
      </div>
  <span class="level_element">Level 75</span>
  <div class="element_desc">
    <div class="top"></div>
    <div class="middle">
      <div class="fix">
        <!--[if !IE]> -->
