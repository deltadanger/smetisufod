<div class="middle_content" id="dague-grouillot">
  <h2 class="title_element" onclick="OpenCloseDiv('bb11357');">Dague de Grouillot</h2>  <div id="bb11357" class="forumbbcode" style="display:none;">
    <div class="h3" style="color:#000 !important">Forum :</div>
    <p><input onclick="javascript:this.select();" type="text" readonly="readonly" value="[dofusitem=11357]Dague de Grouillot[/dofusitem]" /></p>
      </div>
  <span class="level_element">Level 45</span>
  <div class="element_desc">
    <div class="top"></div>
    <div class="middle">
      <div class="fix">
        <!--[if !IE]> -->
<object type="application/x-shockwave-flash" data="http://staticns.ankama.com/dofus/www//game/objectViewer.swf" id="F8e427e4cb3d1b9104394aff92b9f9f25" width="120" height="120">
<!-- <![endif]-->
<!--[if IE]>
<object class="fix" classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version="10.0.0 id="F8e427e4cb3d1b9104394aff92b9f9f25" width="120" height="120">
  <param name="movie" value="http://staticns.ankama.com/dofus/www//game/objectViewer.swf" />
  <param name="allowscriptaccess" value="always" />
  <param name="wmode" value="transparent" />
