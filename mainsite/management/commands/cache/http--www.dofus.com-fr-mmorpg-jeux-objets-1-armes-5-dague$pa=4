<div class="middle_content" id="dague-haih-ri-don">
  <h2 class="title_element" onclick="OpenCloseDiv('bb9716');">Dague Haih'Ri'Don'</h2>  <div id="bb9716" class="forumbbcode" style="display:none;">
    <div class="h3" style="color:#000 !important">Forum :</div>
    <p><input onclick="javascript:this.select();" type="text" readonly="readonly" value="[dofusitem=9716]Dague Haih'Ri'Don'[/dofusitem]" /></p>
      </div>
  <span class="level_element">Level 60</span>
  <div class="element_desc">
    <div class="top"></div>
    <div class="middle">
      <div class="fix">
        <!--[if !IE]> -->
<object type="application/x-shockwave-flash" data="http://staticns.ankama.com/dofus/www//game/objectViewer.swf" id="Ffc38467be861ec4960dc7cdc7dc150f9" width="120" height="120">
