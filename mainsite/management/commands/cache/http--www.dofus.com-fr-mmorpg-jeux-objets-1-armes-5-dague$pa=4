<div class="middle_content" id="dagues-aerdala">
  <h2 class="title_element" onclick="OpenCloseDiv('bb7256');">Dagues Aerdala</h2>  <div id="bb7256" class="forumbbcode" style="display:none;">
    <div class="h3" style="color:#000 !important">Forum :</div>
    <p><input onclick="javascript:this.select();" type="text" readonly="readonly" value="[dofusitem=7256]Dagues Aerdala[/dofusitem]" /></p>
      </div>
  <span class="level_element">Level 62</span>
  <div class="element_desc">
    <div class="top"></div>
    <div class="middle">
      <div class="fix">
        <!--[if !IE]> -->
<object type="application/x-shockwave-flash" data="http://staticns.ankama.com/dofus/www//game/objectViewer.swf" id="F75e25e7f01ce80ce0ca0cbc8988e0211" width="120" height="120">
