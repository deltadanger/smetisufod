<div class="middle_content" id="siroko">
  <h2 class="title_element" onclick="OpenCloseDiv('bb14833');">Siroko</h2>  <div id="bb14833" class="forumbbcode" style="display:none;">
    <div class="h3" style="color:#000 !important">Forum :</div>
    <p><input onclick="javascript:this.select();" type="text" readonly="readonly" value="[dofusitem=14833]Siroko[/dofusitem]" /></p>
      </div>
  <span class="level_element">Level 60</span>
  <div class="element_desc">
    <div class="top"></div>
    <div class="middle">
      <div class="fix">
        <!--[if !IE]> -->
<object type="application/x-shockwave-flash" data="http://staticns.ankama.com/dofus/www//game/objectViewer.swf" id="F3451c380da0272a570a37ba05c6c1cfb" width="120" height="120">
