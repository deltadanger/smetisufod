<div class="middle_content" id="fourbasse-ton">
  <h2 class="title_element" onclick="OpenCloseDiv('bb6762');">Fourbasse-Ton</h2>  <div id="bb6762" class="forumbbcode" style="display:none;">
    <div class="h3" style="color:#000 !important">Forum :</div>
    <p><input onclick="javascript:this.select();" type="text" readonly="readonly" value="[dofusitem=6762]Fourbasse-Ton[/dofusitem]" /></p>
      </div>
  <span class="level_element">Level 32</span>
  <div class="element_desc">
    <div class="top"></div>
    <div class="middle">
      <div class="fix">
        <!--[if !IE]> -->
<object type="application/x-shockwave-flash" data="http://staticns.ankama.com/dofus/www//game/objectViewer.swf" id="Fb8726408b9fc7e1f88b93868ed0c692d" width="120" height="120">
<!-- <![endif]-->
<!--[if IE]>
<object class="fix" classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version="10.0.0 id="Fb8726408b9fc7e1f88b93868ed0c692d" width="120" height="120">
  <param name="movie" value="http://staticns.ankama.com/dofus/www//game/objectViewer.swf" />
  <param name="allowscriptaccess" value="always" />
