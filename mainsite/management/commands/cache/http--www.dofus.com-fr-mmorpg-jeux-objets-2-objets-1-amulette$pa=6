                      <li class="list0">1 PA</li>
                        <li class="list1">31 à 50 Agilité</li>
                        <li class="list0">31 à 50 Chance</li>
                        <li class="list1">31 à 50 Force</li>
                        <li class="list0">31 à 50 Intelligence</li>
                        <li class="list1">6 à 8 % Résistance Air</li>
                        <li class="list0">6 à 8 % Résistance Eau</li>
                        <li class="list1">6 à 8 % Résistance Feu</li>
                        <li class="list0">6 à 8 % Résistance Terre</li>
                        <li class="list1">1 PO</li>
                        <li class="list0">3 Dommages Air</li>
                        <li class="list1">3 Dommages Eau</li>
                        <li class="list0">3 Dommages Feu</li>
                        <li class="list1">3 Dommages Terre</li>
                        <li class="list0">Titre : Maître des Éléments</li>
                      </ul>
              </div>
      <div class="element_carac_right">
