<div class="middle_content" id="torque-ancestral">
  <h2 class="title_element" onclick="OpenCloseDiv('bb8465');">Torque Ancestral</h2>  <div id="bb8465" class="forumbbcode" style="display:none;">
    <div class="h3" style="color:#000 !important">Forum :</div>
    <p><input onclick="javascript:this.select();" type="text" readonly="readonly" value="[dofusitem=8465]Torque Ancestral[/dofusitem]" /></p>
      </div>
  <span class="level_element">Level 110</span>
  <div class="element_desc">
    <div class="top"></div>
    <div class="middle">
      <div class="fix">
        <!--[if !IE]> -->
<object type="application/x-shockwave-flash" data="http://staticns.ankama.com/dofus/www//game/objectViewer.swf" id="F7f5e7c0d877257e5772e0e4e7f189289" width="120" height="120">
