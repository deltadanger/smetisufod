<div class="middle_content" id="brillante-etoile-soir">
  <h2 class="title_element" onclick="OpenCloseDiv('bb1638');">Brillante Etoile du soir</h2>  <div id="bb1638" class="forumbbcode" style="display:none;">
    <div class="h3" style="color:#000 !important">Forum :</div>
    <p><input onclick="javascript:this.select();" type="text" readonly="readonly" value="[dofusitem=1638]Brillante Etoile du soir[/dofusitem]" /></p>
      </div>
  <span class="level_element">Level 48</span>
  <div class="element_desc">
    <div class="top"></div>
    <div class="middle">
      <div class="fix">
        <!--[if !IE]> -->
<object type="application/x-shockwave-flash" data="http://staticns.ankama.com/dofus/www//game/objectViewer.swf" id="F35d4af41a3f92b61d6991611f9d22536" width="120" height="120">
