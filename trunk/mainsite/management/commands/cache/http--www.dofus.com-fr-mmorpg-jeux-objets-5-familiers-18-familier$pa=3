    <p><input onclick="javascript:this.select();" type="text" readonly="readonly" value="[dofusitem=10657]Chercheur d'Ogrines[/dofusitem]" /></p>
      </div>
  <span class="level_element">Level 1</span>
  <div class="element_desc">
    <div class="top"></div>
    <div class="middle">
      <div class="fix">
        <!--[if !IE]> -->
<object type="application/x-shockwave-flash" data="http://staticns.ankama.com/dofus/www//game/objectViewer.swf" id="Feabe902f355ed88591648de4f6c74c87" width="120" height="120">
