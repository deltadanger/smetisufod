<div class="middle_content" id="bottes-gardien-egouts">
  <h2 class="title_element" onclick="OpenCloseDiv('bb6654');">Bottes du Gardien des Égouts</h2>  <div id="bb6654" class="forumbbcode" style="display:none;">
    <div class="h3" style="color:#000 !important">Forum :</div>
    <p><input onclick="javascript:this.select();" type="text" readonly="readonly" value="[dofusitem=6654]Bottes du Gardien des Égouts[/dofusitem]" /></p>
      </div>
  <span class="level_element">Level 83</span>
  <div class="element_desc">
    <div class="top"></div>
    <div class="middle">
      <div class="fix">
        <!--[if !IE]> -->
<object type="application/x-shockwave-flash" data="http://staticns.ankama.com/dofus/www//game/objectViewer.swf" id="F40e14f8925c7c9cd1567a0da758156af" width="120" height="120">
